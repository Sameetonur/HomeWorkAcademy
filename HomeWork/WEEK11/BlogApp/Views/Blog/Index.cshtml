@{
    ViewData["Title"] = "Index";
}
<h1>Yazılar Sayfası</h1>

@foreach (var blogs in (List<BlogModel>)ViewData["BlogModelList"])
{
    <div class="container">
        <div class="card mb-3 col-3">
            <img src="@blogs.Resim" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">@blogs.Baslik</h5>
                <p class="card-text">@blogs.Aciklama</p>
                <p class="card-text"><small class="text-body-secondary">@DateTime.Now</small></p>
            </div>
        </div>
    </div>



}
